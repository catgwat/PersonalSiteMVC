@{
    ViewData["Home"] = "SAT Details";
}
<main id="main">

    <!-- ======= Breadcrumbs ======= -->
    <section id="breadcrumbs" class="breadcrumbs">
        <div class="breadcrumb-hero">
            <div class="container">
                <div class="breadcrumb-hero">
                    <h2>SAT Details</h2>
                    <p>Paired Programming Full Stack Web Application </p>
                </div>
            </div>
        </div>
        <div class="container">
            <ol>
                <li>@Html.ActionLink("Home", "Index", "Home")</li>
                <li>SAT Details</li>
            </ol>
        </div>
    </section><!-- End Breadcrumbs -->
    <!-- ======= Portfolio Details Section ======= -->
    <section id="portfolio-details" class="portfolio-details">
        <div class="container">

            <div class="row gy-4">

                <div class="col-lg-8">
                    <div class="portfolio-details-slider swiper">
                        <div class="swiper-wrapper align-items-center">

                            <div class="swiper-slide">
                                <img src="~/assets/img/portfolio/SAT1.png" alt="figures posed like they're working construction">
                            </div>

                            <div class="swiper-slide">
                                <img src="~/assets/img/portfolio/SAT2.png" alt="figures posed like they're working construction">
                            </div>

                            <div class="swiper-slide">
                                <img src="~/assets/img/portfolio/SATCode.png" alt="figures posed like they're working construction">
                            </div>

                        </div>
                        <div class="swiper-pagination"></div>
                    </div>
                </div>

                <div class="col-lg-4">
                    <div class="portfolio-info">
                        <h3>Project information</h3>
                        <ul>
                            <li><strong>Category</strong>: Full Stack</li>
                            <li><strong>Program</strong>: SQL, .NET MVC, Paired Programming</li>
                            <li><strong>Project date</strong>: May 3, 2024</li>
                            <li><strong>Github Repo Link</strong>: <a href="https://github.com/catgwat/SAT-Project">https://github.com/catgwat/SAT-Project</a></li>
                            <li><strong>Partner</strong>: <a href="http://www.ethanjamesaa.com/">Ethan Anderson-Alexander</a></li>
                        </ul>
                    </div>
                    <div class="portfolio-description">
                        <h2>SAT Portfolio Detail</h2>
                        <p>
                            For this project, we performed Paired Programming and utilized Agile/Scrum principles to mimic a real world "sprint" to complete an application. This is a secure application for managing data. The application is built to simulate an online school system. Administrators will have the ability to manage students, courses, scheduled classes, and enrollments with full CRUD functionality.
                        </p>
                    </div>
                </div>

            </div>

        </div>
    </section><!-- End Portfolio Details Section -->

</main><!-- End #main -->
